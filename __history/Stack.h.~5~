#include <set>



class OPZ
{
	public:
    struct Stack
{
	char data;
	Stack *next;
};
	Stack *begin;
	Stack *InStack(Stack *, char);       //запись в стек
	Stack *OutStack(Stack *, char *);    //извлечение из стека (с удалением)
	int Priority (char);                 //приоритет операций при подсчете
	double Rezult(String);               //подсчет значения выражения
	double array[201];
	//Set <char, 0, 255> sign;             //контейнер для множества символов знаков
	int Ammount = 6;
};

